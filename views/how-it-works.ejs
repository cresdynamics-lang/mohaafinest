    margin-bottom: 80px;
    padding: 40px;
    background: var(--light-grey);
    border-radius: 12px;
}

.process-step-large.alt {
    grid-template-columns: 300px 1fr 80px;
}

.step-icon {
    width: 80px;
    height: 80px;
    background: var(--accent-gold);
    color: var(--white);
    border-radius: 50%;
